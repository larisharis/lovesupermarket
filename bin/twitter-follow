#!/var/web_apps/lovesupermarket/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'twitter==1.9.0','console_scripts','twitter-follow'
__requires__ = 'twitter==1.9.0'
import sys
from pkg_resources import load_entry_point

sys.exit(
   load_entry_point('twitter==1.9.0', 'console_scripts', 'twitter-follow')()
)
